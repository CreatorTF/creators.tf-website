<div
    class="node[INACTIVE] inactive[/INACTIVE]"
    style="left: calc({posX} - 150px); top: calc({posY} - 50px);"
    onclick="CEContracker_GotoNamedNode('{title}')"
    data-title="{title}"

    data-posx="{posX}"
    data-posy="{posY}"
>
    <div class="node-icon"></div>
    <div class="node-name">{name}</div>
    <div class="node-label">Contains {count} contracts</div>
</div>
