<div class="flex store-checkout-listing">
  <div class="noshrink">
    <img src="{image}" style="width: 64px" alt="">
  </div>
  <div class="noshrink store-checkout-counter p-l-10 p-r-5">
    <input class="input" min="1" type="number" name="" value="{count}">
  </div>
  <div class="fullwidth tf2-build store-checkout-name p-l-10 store-checkout-text" style="color: #FFD700">{name}</div>
  <div class="noshrink store-checkout-remove p-l-5">
    <div class="tf-button">Remove</div>
  </div>
  <div class="noshrink store-checkout-price tf2-build font-size-20 color-999 store-checkout-text">
    {price} <i class="mdi mdi-currency-usd-circle-outline"></i>
  </div>
</div>
